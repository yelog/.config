{
  "mcpServers": {
    "puppeteer": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-puppeteer"
      ]
    },
    "git": {
      "args": [
        "mcp-server-git",
        "--repository",
        "/path/to/git/repo"
      ],
      "command": "uvx",
      "disabled": true
    },
    "apple-mcp": {
      "args": [
        "--no-cache",
        "apple-mcp@latest"
      ],
      "command": "bunx",
      "disabled": true
    },
    "postgres": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-postgres",
        "postgresql://a_appconnect:a_appconnect@1234@10.196.174.39:5432/moss_biz?currentSchema=tools"
      ]
    }
  }
}
